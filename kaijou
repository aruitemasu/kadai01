import sys


def factorial(n):
    """階乗を計算する関数"""
    if n < 0:
        return "負の数には階乗の定義がありません。"
    elif n == 0 or n == 1:
        return 1
    else:
        result = 1
        for i in range(2, n + 1):
            result *= i
        return result


def main():
    """標準入力から数値を受け取り、階乗を計算"""
    try:
        # 標準入力を受け取る
        input_data = sys.stdin.read().strip()

        # 数値に変換できなければエラー
        number = int(input_data)

        #階乗を計算して出力
        if number < 0:
            print("負の数には階乗の定義がありません。")
        else:
            print(f"{number} の階乗は{factorial(number)}")
    except ValueError:
        print("整数を入力してください。")
        sys.exit(1)


if __name__ == "__main__":
    main()
